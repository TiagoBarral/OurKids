<div class="container">
  <%= simple_form_for [@family, @child] do |f| %>
    <%= f.input :first_name, required: true %>
    <%= f.input :last_name, required: true %>
    <%= f.input :photo, required: true, class: "upload-picture" %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.button :submit, "Submit" %>
  <% end %>
</div>
