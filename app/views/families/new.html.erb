<div class="kids">
  <h2 class="text-align-left" style="margin-left:7%;">Enroll a Co-Parent</h2>
  <div class="thin-div">
    <div class="large-div">
    </div>
  </div>

  <div class= "family-dashboard">
    <%= form_for :coparent, url: families_coparent_path, method: :post do |f| %>
    CoParent name: <%= f.text_field :coparent_name %><br />
    CoParent email : <%= f.text_field :coparent_email %><br />
    <%= f.submit %>
    <% end %>
  </div>
</div>
